{
    "manifest_version": 2,
    "name": "HideHNComments",
    "version": "0.2",
    "description": "HN comments are distracting. Hide them.",
    "content_scripts": [
        {
        "matches": ["http://news.ycombinator.com/*","https://news.ycombinator.com/*"],
        "js": ["jquery.js", "hide.js"]
        }
    ]
}

